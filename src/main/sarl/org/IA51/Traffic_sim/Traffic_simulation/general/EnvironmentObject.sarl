package org.IA51.Traffic_sim.Traffic_simulation.general

import org.IA51.Traffic_sim.Traffic_simulation.labs.Point2f

/** 
 * @author matthieu
 * 
 */
class EnvironmentObject {
		var length : float
		var position : Point2f
		var UID = 0//UUID::randomUUID
		
	def getLength() {
		return this.length
	}

	def setLength(x : float) {
		this.length = x
	}

	def getPosition() : Point2f {
		return this.position
	}

	def setPosition(pos : Point2f) {
		this.position = pos
	}
}
